{
  "name": "mypage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ts": "node --loader ts-node/esm",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "start": "next dev --port 8888",
    "start:dev": "next dev --port 3000",
    "serve": "next start --port 3000",
    "webp": "npm pkg set 'type'='module' && yarn ts ./src/webp.ts && npm pkg delete 'type'",
    "webp:mv": "npm pkg set 'type'='module' && yarn ts ./src/webp.ts && npm pkg delete 'type' && mv ./public/myproject/new/* ./public/myproject/"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.17",
    "next": "13.4.17",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/imagemin-webp": "^7.0.3",
    "autoprefixer": "^10.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1"
  }
}
